                        "description": "Slug travel",
                        "required": true,
                        "example": "aut-totam"
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "The page number to retrieve",
                        "required": false,
                        "example": "1"
                    },
                    {
                        "name": "start_date",
                        "in": "query",
                        "description": "The Start Date tours",
                        "required": false,
                        "example": "2025-02-09"
                    },
                    {
                        "name": "end_date",
                        "in": "query",
                        "description": "The End Date tours",
                        "required": false,
                        "example": "2025-02-15"
                    },
                    {
                        "name": "price_from",
                        "in": "query",
                        "description": "The price from tours",
                        "required": false,
                        "example": "204.32"
